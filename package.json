{
  "name": "@custom-js/n8n-nodes-pdf-toolkit",
  "version": "1.49.0",
  "description": "This is official node for interacting with APIs from customjs.space",
  "keywords": [
    "customjs",
    "pdf",
    "n8n-community-node-package",
    "pdf-toolkit"
  ],
  "homepage": "https://www.customjs.space/",
  "repository": {
    "type": "git",
    "url": "https://github.com/customjs/n8n-nodes-pdf-toolkit.git"
  },
  "license": "MIT",
  "author": {
    "name": "Henrik Lippke",
    "email": "h@customjs.io"
  },
  "files": [
    "dist",
    "credentials"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CustomJsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Scraper/Scraper.node.js",
      "dist/nodes/SSLChecker/SSLChecker.node.js",
      "dist/nodes/Html2Pdf/Html2Pdf.node.js",
      "dist/nodes/WebsiteScreenshot/WebsiteScreenshot.node.js",
      "dist/nodes/MergePdfs/MergePdfs.node.js",
      "dist/nodes/PdfToText/PdfToText.node.js",
      "dist/nodes/PdfToPng/PdfToPng.node.js",
      "dist/nodes/ExtractPages/ExtractPages.node.js",
      "dist/nodes/CompressPDF/CompressPDF.node.js",
      "dist/nodes/Markdown2Html/Markdown2Html.node.js",
      "dist/nodes/Html2Docx/Html2Docx.node.js",
      "dist/nodes/GetFormFieldNames/GetFormFieldNames.node.js",
      "dist/nodes/PdfFormFill/PdfFormFill.node.js",
      "dist/nodes/InvoiceGenerator/InvoiceGenerator.node.js"
    ]
  },
  "devDependencies": {
    "typescript": "~4.8.4",
    "gulp": "^4.0.2",
    "@types/node": "^16.11.7"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write ."
  }
}