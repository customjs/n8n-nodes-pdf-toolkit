{
  "name": "@custom-js/n8n-nodes-pdf-toolkit",
  "version": "1.11.0",
  "description": "This is official node for interacting with APIs from customjs.space",
  "keywords": [
    "customjs",
    "pdf",
    "n8n-community-node-package",
    "pdf-toolkit"
  ],
  "homepage": "https://www.customjs.space/",
  "bugs": {
    "url": "https://github.com/customjs/n8n-nodes-pdf-toolkit/issues"
  },
  "license": "ISC",
  "author": "Julian Szuper",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CustomJsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Scraper/Scraper.node.js",
      "dist/nodes/SSLChecker/SSLChecker.node.js",
      "dist/nodes/Html2Pdf/Html2Pdf.node.js",
      "dist/nodes/WebsiteScreenshot/WebsiteScreenshot.node.js",
      "dist/nodes/MergePdfs/MergePdfs.node.js",
      "dist/nodes/PdfToText/PdfToText.node.js",
      "dist/nodes/PdfToPng/PdfToPng.node.js",
      "dist/nodes/ExtractPages/ExtractPages.node.js",
      "dist/nodes/CompressPDF/CompressPDF.node.js"
    ]
  },
  "devDependencies": {
    "n8n-workflow": "^0.141.0",
    "typescript": "~4.8.4",
    "gulp": "^4.0.2",
    "@types/node": "^16.11.7"
  },
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write ."
  }
}
